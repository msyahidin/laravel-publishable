{
  "name": "bmatovu/laravel-publishable",
  "description": "Laravel publisable.",
  "homepage": "https://github.com/mtvbrianking/laravel-publishable",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "lumen",
    "eloquent",
    "model",
    "publishable",
    "trait",
    "package"
  ],
  "authors": [
    {
      "name": "Brian Matovu",
      "email": "mtvbrianking@gmail.com",
      "homepage": "http://bmatovu.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "illuminate/database": "^9.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "illuminate/support": "^9.0",
    "illuminate/queue": "^9.0",
    "orchestra/testbench": "^7.1",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Bmatovu\\Publishable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bmatovu\\Publishable\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "doc": "php sami.phar update .sami.dist",
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  }
}
